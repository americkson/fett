name = Fett
description = [Foundation] [SASS] This is my face...
screenshot = assets/images/screenshot.png
core = 7.x
engine = phptemplate
version = "7.x-3.0"
php = 5.2


;-------------- FEATURES
features[] = logo
features[] = name
features[] = slogan
features[] = main_menu
features[] = secondary_menu


;-------------- REGIONS
regions[header]         = Header
regions[navigation]     = Navigation
regions[help]           = Help
regions[featured]       = Featured
regions[content]        = Content
regions[sidebar_first]  = First Sidebar
regions[sidebar_second] = Second Sidebar
regions[footer]         = Footer


;-------------- STYLESHEETS
;stylesheets[all][] = assets/scss/none.scss


;-------------- CORE STYLESHEETS
; These differ from regular stylesheets as they are loaded into CSS_DEFAULT
; instead of CSS_THEME which allows them to be altered later via Sonar.
scss[] = assets/scss/_variables.scss
scss[] = assets/scss/_mixins.scss
scss[] = assets/scss/_core.scss
scss[] = assets/scss/_forms.scss
scss[] = assets/scss/_contextual.scss


;-------------- MODULE STYLESHEETS
; These stylesheets are loaded if their module [key] exists.
scss_module[betterlogin] = assets/scss/modules/_betterlogin.scss
scss_module[linkit] = assets/scss/modules/_linkit.scss
scss_module[flexslider] = assets/scss/modules/_flexslider.scss
scss_module[views] = assets/scss/modules/_views.scss
scss_module[webform_multifile] = assets/scss/modules/_webform_multifile.scss


;-------------- JAVASCRIPT
; scripts[] = assets/js/none.js


;-------------- DEFAULTS
settings[html_tags]               = 1
settings[css_onefile]             = 1
settings[js_onefile]              = 1
settings[css_exclude]             = 1
settings[favicons_default_remove] = 1
settings[jquery_update_jquery_version] = '1.10'
; Foundation CSS
settings[css_foundation][]   = 'grid'
settings[css_foundation][]   = 'block-grid'
settings[css_foundation][]   = 'breadcrumbs'
settings[css_foundation][]   = 'button-groups'
settings[css_foundation][]   = 'buttons'
settings[css_foundation][]   = 'flex-video'
settings[css_foundation][]   = 'forms'
settings[css_foundation][]   = 'pagination'
settings[css_foundation][]   = 'split-buttons'
settings[css_foundation][]   = 'tables'
settings[css_foundation][]   = 'type'
settings[css_foundation][]   = 'visibility'


;-------------- FORM ALTER
; form_alter[] = 'user_login'


;-------------- GLOBAL INCLUDES
; These are files that will be included on each page and exist in /includes
include_global[] = 'tools'
include_global[] = 'css'
include_global[] = 'js'
include_global[] = 'foundation'
include_global[] = 'tooltip'
include_global[] = 'form'
include_global[] = 'theme'
include_global[] = 'offcanvas'
include_global[] = 'favicons'


;-------------- VARIABLE INCLUDES
; These are files that will be included when a fett_get_settings(setting) is
; TRUE and exist in /includes
include_var[] = 'megamenu'
include_var[] = 'share'


;-------------- PREPROCESS INCLUDES
; These are files that will be included for preprocess operations and exist
; in /preprocess
include_preprocess[] = 'html'
include_preprocess[] = 'page'
include_preprocess[] = 'entity'
include_preprocess[] = 'node'
include_preprocess[] = 'block'
include_preprocess[] = 'field'
include_preprocess[] = 'video_embed_field_embed_code'
include_preprocess[] = 'views_view'
include_preprocess[] = 'views_view_unformatted'
include_preprocess[] = 'webform_element'
include_preprocess[] = 'user_profile'


;-------------- PROCESS INCLUDES
; These are files that will be included for process operations and exist
; in /preprocess
include_process[] = 'html'
include_process[] = 'html_tag'
include_process[] = 'page'


;-------------- CRAWL CSS
; Folders within sites/all/modules that should be crawled for CSS files that
; can be excluded via the theme settings page. Child themes should define this.
; css_folders_crawl[] = 'contrib'


